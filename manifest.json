{
  "manifest_version": 3,
  "name": "JetSales WhatsApp",
  "version": "1.0",
  "description": "Adiciona logo JetSales e controle de chat ao WhatsApp Web",
  "icons": {
    "48": "assets/jetsales.png",
    "128": "assets/jetchat.png"
  },
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/*"],
      "js": ["main.js"
      ],

"css": ["styles.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/jetsales.png", "chat.png", "assets/jetchat.png","config.json","assets/setting.png"],
      "matches": ["https://web.whatsapp.com/*"]
    }
    
  ],
  "permissions": [
    "http://localhost:4000/",
    "http://localhost:4000/*"
  ]
}