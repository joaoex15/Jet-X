{
  "manifest_version": 3,
  "name": "JetSales WhatsApp",
  "version": "1.0",
  "description": "Adiciona logo JetSales e controle de chat ao WhatsApp Web",
  "icons": {
    "48": "assets/jetsales.png",
    "128": "assets/jetchat.png"
  },
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/*"],
      "js": [
        "content/scriptManager.js",
        "content/uiManager.js",
        "content/chatManager.js",
        "content/content.js"
      ],

"css": [
        "styles/base.css",
        "styles/components.css",
        "styles/floating-elements.css",
        "styles/containers.css",
        "styles/scripts-ui.css",
        "styles/responsive.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/jetsales.png", "chat.png", "assets/jetchat.png","config.json","assets/setting.png"],
      "matches": ["https://web.whatsapp.com/*"]
    }
  ]
}